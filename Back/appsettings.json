{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "AppMvcConnectionString": "Server=localhost; Port=3306; Database=your-db-name-here; user=root;password="
  },
  "Watch": {
    "executablePath": "dotnet.exe",
    "workingDirectory": "$(ProjectDir)",
    "commandLineArgs": "watch run",
    "launchBrowser": false,
    "launchUrl": "https://localhost:5001/",
    "environmentVariables": {
      "ASPNETCORE_ENVIRONMENT": "Development"
    }
  },
  "runtimeOptions": {
    "configProperties": {
      "System.Drawing.EnableUnixSupport": true
    }
  },
  // "JwtIssuer": "https://localhost:5001",
  // "JwtAudience": "https://localhost:3000",
  "JwtSecurityKey": "this is my custom Secret key for authnetication",
  "AccessTokenExpiryInHours": 24,
  "RefreshTokenExpiryInDays": 10,
  "MailSettings": {
    "Mail": "put-your-email@gmail.com",
    "DisplayName": "Lavender",
    "Password": "put-your-email-password-here",
    "Host": "smtp.gmail.com",
    "Port": 587
  }
}

